import sqlite3 as lite
import sys

con = lite.connect("user.db")
#Creates a new database "user.db"

with con: 
    cursor = con.cursor()
    #Creates cursor to point to specific locations in database
    
    cursor.execute("CREATE TABLE Users(Id INT, Name TEXT)")
    cursor.execute("INSERT INTO Users VALUES(1, 'NIGGER_TEJ')")
    cursor.execute("INSERT INTO Users VALUES(2, 'NIGGER_WIN')")
    
    f = cursor.execute("SELECT * FROM Users")
    #returns generator of tuples
    
    for x in f:
        print(x)
    
